{
  "name": "redcare-starhub",
  "version": "0.0.1",
  "description": "List trending GitHub repositories and allow starring and filtering",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3001 --clearScreen false",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc",
    "lint": "eslint src/ --fix",
    "test": "vitest --watch=false",
    "test:coverage": "vitest --coverage.enabled --coverage.clean=true --watch=false",
    "gen:openAPI": "openapi-generator-cli generate -i src/__api__/github.yaml  -g typescript-fetch -o src/__generated__/github --model-name-prefix=I  --additional-properties=modelPropertyNaming=original,enumPropertyNaming=original,supportsES6=true,typescriptThreePlus=true,suppressUnusedParameterWarnings=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/besotti/redcare-starhub.git"
  },
  "author": "Benedikt Sottong <benediktsommer@aol.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/besotti/redcare-starhub/issues"
  },
  "homepage": "https://github.com/besotti/redcare-starhub#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.22.0",
    "@tanstack/react-query": "^5.85.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router": "^5.1.20",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "eslint": "9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "prettier": "^3.6.2",
    "react-intersection-observer": "^9.16.0",
    "save": "^2.9.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.35.1",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "lodash-es": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0"
  }
}
